{
  "files.associations": {
    ".env": "dotenv",
    ".env.local": "dotenv",
    ".env.local.dist": "dotenv"
  },
  "editor.defaultFormatter": "prettier.prettier-vscode",
  "editor.detectIndentation": false,
  "editor.trimAutoWhitespace": true,
  "editor.renderWhitespace": "boundary",
  "editor.renderControlCharacters": false,
  "editor.rulers": [80, 100, 120],
  "editor.wordWrap": "off",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": false,
  "editor.formatOnType": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "files.insertFinalNewline": false,
  "files.trimTrailingWhitespace": true,
  "files.eol": "\n",
  "[json]": {
    "editor.defaultFormatter": "prettier.prettier-vscode",
    "editor.tabSize": 2,
    "editor.indentSize": 2,
    "editor.insertSpaces": true,
    "files.insertFinalNewline": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "prettier.prettier-vscode",
    "editor.tabSize": 2,
    "editor.indentSize": 2,
    "editor.insertSpaces": true,
    "files.insertFinalNewline": true
  },
  "[yaml]": {
    "editor.defaultFormatter": "prettier.prettier-vscode",
    "editor.tabSize": 2,
    "editor.indentSize": 2,
    "editor.insertSpaces": true,
    "files.insertFinalNewline": true
  },
  "[xml]": {
    "editor.defaultFormatter": "prettier.prettier-vscode",
    "editor.tabSize": 2,
    "editor.indentSize": 2,
    "editor.insertSpaces": true,
    "files.insertFinalNewline": true
  },
  "[html]": {
    "editor.defaultFormatter": "prettier.prettier-vscode",
    "editor.tabSize": 2,
    "editor.indentSize": 2,
    "editor.insertSpaces": true,
    "files.insertFinalNewline": true
  },
  "[css]": {
    "editor.defaultFormatter": "prettier.prettier-vscode",
    "editor.tabSize": 2,
    "editor.indentSize": 2,
    "editor.insertSpaces": true,
    "files.insertFinalNewline": true
  },
  "[scss]": {
    "editor.defaultFormatter": "prettier.prettier-vscode",
    "editor.tabSize": 2,
    "editor.indentSize": 2,
    "editor.insertSpaces": true,
    "files.insertFinalNewline": true
  },
  "[svg]": {
    "editor.defaultFormatter": "prettier.prettier-vscode",
    "editor.tabSize": 2,
    "editor.indentSize": 2,
    "editor.insertSpaces": true,
    "files.insertFinalNewline": true
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "prettier.prettier-vscode",
    "editor.tabSize": 4,
    "editor.indentSize": 4,
    "editor.insertSpaces": true,
    "files.insertFinalNewline": true
  },
  "[shellscript]": {
    "editor.defaultFormatter": "prettier.prettier-vscode",
    "editor.tabSize": 4,
    "editor.indentSize": 4,
    "editor.insertSpaces": true,
    "files.insertFinalNewline": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "prettier.prettier-vscode",
    "editor.tabSize": 2,
    "editor.indentSize": 2,
    "editor.insertSpaces": true,
    "files.insertFinalNewline": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "prettier.prettier-vscode",
    "editor.tabSize": 4,
    "editor.indentSize": 4,
    "editor.insertSpaces": true,
    "files.insertFinalNewline": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "prettier.prettier-vscode",
    "editor.tabSize": 4,
    "editor.indentSize": 4,
    "editor.insertSpaces": false,
    "files.insertFinalNewline": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "prettier.prettier-vscode",
    "editor.tabSize": 4,
    "editor.indentSize": 4,
    "editor.insertSpaces": true,
    "files.insertFinalNewline": true
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "prettier.prettier-vscode",
    "editor.tabSize": 4,
    "editor.indentSize": 4,
    "editor.insertSpaces": true,
    "files.insertFinalNewline": true
  },
  "[vue]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "makefile.configureOnOpen": false,
  "prettier.requireConfig": true,
  "vue.autoInsert.bracketSpacing": true,
  "vue.autoInsert.dotValue": false,
  "vue.codeActions.askNewComponentName": true,
  "vue.editor.focusMode": false,
  "vue.editor.reactivityVisualization": true,
  "vue.editor.templateInterpolationDecorators": true,
  "vue.format.script.enabled": true,
  "vue.format.script.initialIndent": false,
  "vue.format.style.enabled": true,
  "vue.format.style.initialIndent": false,
  "vue.format.template.enabled": true,
  "vue.format.template.initialIndent": true,
  "vue.format.wrapAttributes": "auto",
  "vue.hover.rich": false,
  "vue.inlayHints.destructuredProps": false,
  "vue.inlayHints.inlineHandlerLeading": false,
  "vue.inlayHints.missingProps": false,
  "vue.inlayHints.optionsWrapper": false,
  "vue.inlayHints.vBindShorthand": false,
  "vue.server.includeLanguages": ["vue"],
  "vue.server.path": "",
  "vue.suggest.componentNameCasing": "preferPascalCase",
  "vue.suggest.defineAssignment": true,
  "eslint.format.enable": true,
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "typescript.tsdk": "node_modules/typescript/lib"
}
